{
  "name": "aoux/system-monitor",
  "description": "Advanced system monitoring package for Laravel applications",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mert Ülkü",
      "email": "mertsmulku@gmail.com"
    }
  ],
  "homepage": "https://github.com/KULLANICI_ADINIZ/system-monitor",
  "keywords": [
    "laravel",
    "monitor",
    "cache",
    "queue",
    "redis",
    "system",
    "performance"
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Aoux\\SystemMonitor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aoux\\SystemMonitor\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aoux\\SystemMonitor\\SystemMonitorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}